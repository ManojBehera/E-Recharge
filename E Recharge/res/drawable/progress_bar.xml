<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="@dimen/dpb_rounded_corners" />
            <solid android:color="@color/silver_cloud" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dpb_rounded_corners" />
                <solid android:color="@color/regatta" />
            </shape>
        </clip>
    </item>
</layer-list>